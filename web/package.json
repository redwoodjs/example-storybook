{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "browserslist": {
    "development": [
      "last 1 version"
    ],
    "production": [
      "defaults",
      "not IE 11",
      "not IE_Mob 11"
    ]
  },
  "dependencies": {
    "@redwoodjs/auth": "1.0.0-rc.final.3",
    "@redwoodjs/forms": "1.0.0-rc.final.3",
    "@redwoodjs/router": "1.0.0-rc.final.3",
    "@redwoodjs/web": "1.0.0-rc.final.3",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.4.20",
    "@storybook/addon-interactions": "6.4.20",
    "@storybook/jest": "0.0.10",
    "@storybook/test-runner": "0.0.4",
    "@storybook/testing-library": "0.0.9",
    "autoprefixer": "10.4.4",
    "jest": "27.5.1",
    "postcss": "8.4.12",
    "postcss-loader": "6.2.1",
    "tailwindcss": "3.0.23"
  },
  "scripts": {
    "test-storybook": "test-storybook --url http://localhost:7910/ --config-dir \"${PWD}/../node_modules/@redwoodjs/testing/config/storybook/\"",
    "test-storybook:ci": "npx concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"yarn rw sb\" \"npx wait-on tcp:7910 && yarn test-storybook\""
  }
}
